<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.website.demo.dao.Student" table="student" catalog="website">
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="firstName" type="string">
            <column name="STOCK_CODE" length="20" not-null="true"/>
        </property>
        <property name="lastName" type="string">
            <column name="STOCK_NAME" length="20" not-null="true"/>
        </property>
        <property name="age" type="java.lang.Integer">
            <column name="AGE" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>